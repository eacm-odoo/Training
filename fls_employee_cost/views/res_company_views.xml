<odoo>
    <record model="ir.ui.view" id="view_company_form_inherit_fls_employee_cost">
        <field name="name">res.company.form.inherit.fls.emloyee.cost</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='favicon']" position="after">
                <field name="annual_hours" widget="float_time"/>
            </xpath>
        </field>
    </record>
</odoo>
