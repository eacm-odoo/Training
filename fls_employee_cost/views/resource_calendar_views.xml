<odoo>
    <record model="ir.ui.view" id="resource_calendar_form_inherit_fls_employee_cost">
        <field name="name">resource.calendar.form.inherit.fls.emloyee.cost</field>
        <field name="model">resource.calendar</field>
        <field name="inherit_id" ref="resource.resource_calendar_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='hours_per_day']" position="after">
                <field name="week_hours" widget="float_time"/>
            </xpath>
        </field>
    </record>
</odoo>
