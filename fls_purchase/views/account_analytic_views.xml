<odoo>
    <record id="hr_timesheet_line_tree_inherit_fls_purchase" model="ir.ui.view">
        <field name="name">account.analytic.line.tree.hr_timesheet.inherit.fls.purchase</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='employee_id']" position="after">
                <field name="recent_contract_id"/>
            </xpath>
        </field>
    </record>
</odoo>